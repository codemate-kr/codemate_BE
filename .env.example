# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/codemate?useSSL=false&serverTimezone=Asia/Seoul&characterEncoding=UTF-8
DB_USERNAME=root
DB_PASSWORD=root

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
BASE_URL=http://localhost:8080

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-minimum-256-bits

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS Configuration (DefaultCredentialsProvider 사용)
# EC2에서는 IAM 역할 자동 인식, 로컬에서는 아래 환경 변수 필요
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SES_REGION=ap-northeast-2
AWS_SES_FROM_EMAIL=noreply@yourdomain.com
AWS_SES_CONFIGURATION_SET=codemate-tracking

# Cookie Configuration (for production)
COOKIE_DOMAIN=yourdomain.com

# CORS Configuration (for production)
FRONTEND_URL=https://yourdomain.com

# Sentry Configuration
SENTRY_DSN=https://xxxxx@xxxxx.ingest.sentry.io/xxxxx
SENTRY_ENVIRONMENT=local
SENTRY_TRACES_SAMPLE_RATE=1.0